    <style>
      .compatibility-message {
        display: none;
      }
      @media (orientation: portrait) {
        html,
        body {
          overflow: hidden;
          background-color: #000;
          max-width: 100%;
        }
        .compatibility-message {
          display: block;
          position: fixed;
          z-index: 999999;
          width: 100%;
          height: 100%;
          background: #13110e;
          text-align: center;
        }

        .compatibility-message img {
          max-width: 100%;
        }
      }

      /*------------hotsport css-----------------*/
      .hotspots {
        width: 25px;
        height: 25px;
        background-color: #3ab3ef;
        border-radius: 50%;
        position: absolute;
        animation: wave 1s infinite;
        cursor: pointer;
      }
      .digital_booth {
        left: 23.5%;
        bottom: 28.5%;
      }
      .partnership_booth
       {
        left: 48%;
        top: 49%;
      }

      .innovation_booth {
        right: 30%;
        bottom: 29%;
      }

      .hotspots::before {
        content: attr(text);
        width: max-content;
        position: absolute;
        background-color: #222222dd;
        color: #fff;
        left: 50%;
        transform: translateX(-50%);
        top: 30px;
        padding: 8px 20px;
        font-size: 14px;
        border-radius: 3px;
        display: none;
      }

      .hotspots::after {
        content: "";
        border-color: transparent transparent #222222dd;
        border-width: 0 8px 6px;
        border-style: solid;
        position: absolute;
        top: 24px;
        left: 50%;
        transform: translateX(-50%);
        display: none;
      }

      .hotspots:hover::before,
      .hotspots:hover::after {
        display: block;
      }
      @media (max-width: 768px) {
        .hotspots {
          width: 13px;
          height: 13px;
        }
        .digital_booth {
       
        bottom: 30%;
      }

      .innovation_booth {
        
        top: 67%;
      }

      .partnership_booth {
        
        bottom: 32%;
      }

      }
      @media only screen and (min-width: 1280px)  {
        .hotspots {
          width: 25px;
          height: 25px;
        }
        .innovation_booth {
        
        top: 68%;
      }
      }

  @media (max-width:450px){
